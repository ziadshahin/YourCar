<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.ziad.yourcar" >
    <uses-permission android:name="android.permission.CALL_PHONE"></uses-permission>

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >


        <provider
            android:authorities="com.example.ziad.yourcar"
            android:name=".DBProvider"
            android:exported="true"
            android:syncable="true" />
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".Welcome"
            android:label="@string/title_activity_welcome" >
        </activity>
        <activity
            android:name=".Logos"
            android:label="@string/title_activity_logos" >
        </activity>
        <activity
            android:name=".databasenew"
            android:label="@string/title_activity_databasenew" >
        </activity>
    </application>

</manifest>
